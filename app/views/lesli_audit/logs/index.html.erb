<%= render(LesliView::Layout::Container.new("audit-logs")) do %>
    <%= render(LesliView::Components::Header.new("User logs")) %>
    <%= render LesliView::Components::Toolbar.new() %>
    <%= render(LesliView::Elements::Table.new(
        columns: [{
            label: "Module",
            field: "engine"
        },{
            label:"Action",
            field:"action"
        },{
            label:"Operation",
            field:"operation"
        },{
            label:"Description",
            field:"description"
        },{
            label:"Date",
            field:"created_at"
        }],
        records: @logs[:records]
    )) %>
<% end %>
